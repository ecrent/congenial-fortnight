{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "hostRequirements": {
    "cpus": 4
  },
  "waitFor": "onCreateCommand",
  "updateContentCommand": "npm install",
  "postCreateCommand": "",
  "postAttachCommand": {},
  "customizations": {
    "codespaces": {
      "openFiles": [
      
      ]
    }
  },
  "portsAttributes": {
    "3000": {
      "label": "Application",
      "onAutoForward": "openBrowser",
      "visibility": "public",
      "showInExplorer": true
    },
    "5432": {
      "label": "PostgreSQL",
      "onAutoForward": "notify",
      "visibility": "public",
      "showInExplorer": true
    },
    "3001": {
      "label": "React",
      "onAutoForward": "openBrowser",
      "visibility": "public",
      "showInExplorer": true
    }
  },
  "forwardPorts": [3000, 5432, 3001]
}
